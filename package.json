{
  "name": "twitchmoments",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-react\" \"npm run start-flask\" \"npm run start-electron\"",
    "start-react": "cd react-project && npm run dev",
    "start-flask": "cd backend && python twitchMomentsv3.py",
    "start-electron": "electron .",
    "build-react": "cd react-project && npm run build",
    "build": "npm run build-react",
    "postinstall": "npm run build"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "electron": "^31.3.0"
  },
  "dependencies": {
    "electron-log": "^5.1.7",
    "shell": "^0.9.6"
  }
}
